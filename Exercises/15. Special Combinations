function specialCombinations ([p1, p2, p3]){
    p1 = Number(p1);
    p2 = Number(p2);
    p3 = Number(p3);
    for(let i = 2; i <= p1; i+=2){
        for(let j = 0; j <= p2; j++){
            if(j == 2 || j == 3 || j == 5 || j == 7){
                for(let k = 2; k <= p3; k+=2){
                    console.log(`${i} ${j} ${k}`)
                }
            }
        }
    }
}
